<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="doggy.controller.ConfirmController">
   <children>
      <Label fx:id="msg" alignment="CENTER" graphicTextGap="5.0" layoutX="125.0" layoutY="68.0" prefHeight="152.0" prefWidth="350.0" text="Label" textFill="#1e1e1d" wrapText="true">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="223.0" layoutY="277.0" mnemonicParsing="false" onAction="#handelYES" text="Yes" textFill="#726565">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="324.0" layoutY="277.0" mnemonicParsing="false" onAction="#handleNo" text="No">
         <font>
            <Font size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="CLOSED_HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
